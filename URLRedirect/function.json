{
    "scriptFile": "../Modules/entrypoint.psm1",
    "entryPoint": "Receive-HttpTrigger",
    "bindings": [
        {
            "authLevel": "anonymous",
            "type": "httpTrigger",
            "direction": "in",
            "name": "Request",
            "methods": ["get"],
            "route": "{URLslug}"
        },
        {
            "type": "http",
            "direction": "out",
            "name": "Response"
        }
    ]
}